

CREATE COMPUTE MODULE HTTPRouterFlow_mf_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE Id REFERENCE TO InputLocalEnvironment.HTTP.Input.QueryString.Id;
		SET OutputRoot.XMLNSC.Data.Values[] = PASSTHRU('SELECT * FROM EMPLOYEES WHERE I=?'VALUES(Id));
	    RETURN TRUE;
	END;

END MODULE;
