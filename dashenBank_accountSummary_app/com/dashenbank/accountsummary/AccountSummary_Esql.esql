		ELSE
			SET OutputRoot.Properties = InputRoot.Properties;
			SET OutputRoot.HTTPResponseHeader = InputRoot.HTTPResponseHeader;
			CREATE LASTCHILD OF OutputRoot DOMAIN 'SOAP';

			DECLARE inputHeaderRef REFERENCE TO InputRoot.SOAP.Body.*:QUERYACCSUMM_IOFS_RES.*:FCUBS_HEADER;
			DECLARE inputBodyRef REFERENCE TO InputRoot.SOAP.Body.*:QUERYACCSUMM_IOFS_RES.*:FCUBS_BODY;

			--SET OutputRoot.SOAP.Context = InputRoot.SOAP.Context;

			CREATE LASTCHILD OF OutputRoot.SOAP NAME 'Body';
			--CREATE LASTCHILD OF OutputRoot.SOAP.Body  NAME 'QUERYACCSUMM_IOFS_RES' ;
			
			SET OutputRoot.SOAP.Body.ns:QUERYACCSUMM_IOFS_RES.(XMLNSC.NamespaceDecl)xmlns = 'http://fcubs.ofss.com/service/FCUBSAccService'; 
		SET OutputRoot.SOAP.Body.ns:QUERYACCSUMM_IOFS_RES.ns:FCUBS_HEADER.ns:SOURCE = NULLIF(inputHeaderRef.*:SOURCE,'');
			SET OutputRoot.SOAP.Body.ns:QUERYACCSUMM_IOFS_RES.ns:FCUBS_HEADER.ns:UBSCOMP = NULLIF(inputHeaderRef.*:UBSCOMP,'');
			--SET OutputRoot.SOAP.Body.QUERYACCSUMM_IOFS_RES.(XMLNSC.NamespaceDecl)xmlns:ns1 = 'http://www.w3.org/2001/XMLSchema-instance 
			CREATE LASTCHILD OF OutputRoot.SOAP.Body.QUERYACCSUMM_IOFS_RES NAME 'FCUBS_HEADER';
			CREATE LASTCHILD OF OutputRoot.SOAP.Body.QUERYACCSUMM_IOFS_RES NAME 'FCUBS_BODY';
           
			DECLARE outputheaderRef REFERENCE TO  OutputRoot.SOAP.Body.QUERYACCSUMM_IOFS_RES.FCUBS_HEADER;
			DECLARE outputbodyRef REFERENCE TO OutputRoot.SOAP.Body.QUERYACCSUMM_IOFS_RES.FCUBS_BODY;
			
            SET outputheaderRef.SOURCE = NULLIF(inputHeaderRef.*:SOURCE,'');
			SET outputheaderRef.UBSCOMP = NULLIF(inputHeaderRef.*:UBSCOMP,'');
			SET outputheaderRef.MSGID = NULLIF(inputHeaderRef.*:MSGID,'');
