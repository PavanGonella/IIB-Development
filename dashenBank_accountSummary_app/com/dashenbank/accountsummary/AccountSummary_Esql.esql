		--CALL writeToLogFile(MessageFlowLabel,logger_name,log_type,' -------- CHANNEL_RESP -------') INTO rc;
		--CALL writeToLogFile(MessageFlowLabel,logger_name,log_type,'CHANNEL_RESP data is: '||Environment.resPayload) INTO rc;

	END;
