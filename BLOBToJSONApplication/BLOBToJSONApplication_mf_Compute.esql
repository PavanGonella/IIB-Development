

CREATE COMPUTE MODULE BLOBToJSONApplication_mf_Compute
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE Inref CHARACTER CAST(InputRoot.BLOB.BLOB AS CHARACTER ENCODING 546);
		CREATE LASTCHILD OF OutputRoot DOMAIN('JSON') PARSE(Inref OPTIONS RootBitStream); 
		RETURN TRUE;
	END;
	
END MODULE;
