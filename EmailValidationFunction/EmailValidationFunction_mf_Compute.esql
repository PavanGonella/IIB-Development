CREATE PROCEDURE EmailValidate (IN email CHARACTER, OUT isValid BOOLEAN)
BEGIN
	DECLARE query CHARACTER; 
	SET query = PASSTHRU('SELECT CASE WHEN ? REGEXP ''^[a-z0-9.-_]+@[a-z.-]+\.[a-z]{2,}$'' TH;
	SET isValid = REGEXP_LIKE(email, regExp);
	
END;