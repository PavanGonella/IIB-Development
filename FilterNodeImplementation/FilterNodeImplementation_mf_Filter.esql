

CREATE FILTER MODULE FilterNodeImplementation_mf_Filter
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		DECLARE Inref REFERENCE TO Root.XMLNSC.Employee.Details;
		IF  Inref.company = 'TCS' THEN
			RETURN TRUE;
		ELSEIF Inref.company ='Wipro' THEN
			RETURN FALSE;
		ELSE
			RETURN UNKNOWN;
		END IF;
		RETURN TRUE;
	END;

END MODULE;
