{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "Subjects": {
      "type": "object",
      "properties": {
        "Subject": {
          "type": "object",
          "properties": {
            "Name": {
              "type": "string",
              "maxLength": 20
            },
            "Code": {
              "type": "string",
              "minLength": 4
            }
          },
          "required": [
            "Name",
            "Code"
          ]
        }
      },
      "required": [
        "Subject"
      ]
    }
  },
  "required": [
    "Subjects"
  ]
}
